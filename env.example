# GalaSwap Trading Bot Environment Variables
# GalaSwap 트레이딩 봇 환경 변수

# Wallet Configuration / 지갑 설정
WALLET_ADDRESS=your_wallet_address_here
PRIVATE_KEY=your_private_key_here

# GalaChain Configuration / GalaChain 설정
GALA_RPC_URL=https://mainnet.galachain.io
GALA_API_KEY=your_gala_api_key_here

# Trading Configuration / 거래 설정
SIMULATION_MODE=true
MIN_TRADE_AMOUNT=1
MAX_TRADE_AMOUNT=10

# Optional: Advanced Settings / 선택사항: 고급 설정
SLIPPAGE_TOLERANCE=0.5
GAS_LIMIT=300000
GAS_PRICE=20000000000
